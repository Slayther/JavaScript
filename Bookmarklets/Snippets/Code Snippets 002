var videoButtonList = document.getElementsByClassName('yt-lockup-dismissable');

for (var i = 0; videoButtonList[i]; i++) {
	videoButtonList[i].outerHTML = "<div style = \"opacity: .7; background-color: rgba(0,128,0,0.5); outline: 7px solid rgba(0,128,0,0.5);\">" + videoButtonList[i].outerHTML + "</div>";
	
	videoButtonList[i].style.position = "relative";
	videoButtonList[i].style.zIndex = -1;
	
	videoButtonList[i].getElementsByTagName("h3")[0].getElementsByClassName("yt-uix-sessionlink")[0].style.backgroundColor = "transparent";
	
	videoButtonList[i].onmouseenter = function(ev) {
		
	}
	
	videoButtonList[i].onmouseleave = function(ev) {
		
	}
}
