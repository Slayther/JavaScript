var videoButtonList = document.getElementsByClassName('yt-lockup-dismissable');

for (var i = 0; videoButtonList[i]; i++) {
	videoButtonList[i].onmouseenter = function(ev) {
		ev.target.style.backgroundColor = "rgba(0, 206, 0, 0.3)";
		ev.target.style.outline = "7px solid rgba(0, 206, 0, 0.3)";
	}
	
	videoButtonList[i].onmouseleave = function(ev) {
		ev.target.style.backgroundColor = "";
		ev.target.style.outline = "";
	}
}

